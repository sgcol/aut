<script src="/js/client.umd.js"></script>
<script src="https://cdnjs.cloudflare.com/ajax/libs/eruda/1.5.8/eruda.min.js"></script>

<script>
  window.onload=()=>{
    eruda.init();
    (async function run() {
      console.log(<%- JSON.stringify(init_config) %>, <%- JSON.stringify(payData) %>);
      const wechatObj=new WechatJSSDK(<%- JSON.stringify(init_config) %>);
      await wechatObj.initialize();
      wechatObj.callWechatApi('chooseWXPay', Object.assign({
          success: res => {
            console.log(res);
            location.href="<%= return_url%>";
          }
        }, <%-JSON.stringify(payData) %> ));
    })();
  }
</script>